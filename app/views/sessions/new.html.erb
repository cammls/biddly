<h1>Login page</h1>

    <%= form_for(:session, url: login_path) do |f| %>

      <%= f.label :email %>
      <%= f.email_field :email %>

      <%= f.label :password %>
      <%= f.password_field :password %>

    Remember me?  <%= f.check_box :remember_me %>

      <%= f.submit "Log in" %>
    <% end %>
